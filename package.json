{
	"title": "Vite WordPress Vue.js + Tailwind + Inertia.js Starter",
	"name": "wordpress-starter",
	"version": "4.0.1",
	"repository": "git@github.com:kirilldorozhynskyi/jdwordpress.git",
	"license": "GPL-2.0+",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"i18n:add": "vue-i18n-extract report --vueFiles './wp-content/themes/template/resources/Private/Vue/**/**/**/*.?(js|vue)' --languageFiles './wp-content/themes/template/resources/Private/Vue/util/lang/*.?(json|yml|yaml|js|ts)' --add",
		"i18n:remove": "vue-i18n-extract remove --vueFiles './wp-content/themes/template/resources/Private/Vue/**/*.?(js|vue)' --languageFiles './wp-content/themes/template/resources/Private/Vue/util/lang/*.?(json|yml|yaml|js|ts)' --remove"
	},
	"dependencies": {
		"@inertiajs/inertia": "^0.11.1",
		"@inertiajs/vue3": "^2.0.5",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.12",
		"aos": "^3.0.0-beta.6",
		"swiper": "^11.2.5",
		"tailwindcss": "^4.0.12",
		"vanilla-lazyload": "^19.1.3",
		"vue": "^3.5.13",
		"vue-i18n": "^11.1.2",
		"vue-scrollto": "^2.20.0"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.22.4",
		"@spiriit/vite-plugin-svg-spritemap": "^4.0.0",
		"@types/node": "^22.13.10",
		"@vitejs/plugin-vue": "^5.2.1",
		"@zackad/prettier-plugin-twig": "^0.15.3",
		"autoprefixer": "^10.4.21",
		"laravel-vite-plugin": "^1.2.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sass": "^1.85.1",
		"svgo": "^3.3.2",
		"vite": "^6.2.1",
		"vite-plugin-mkcert": "^1.17.7",
		"vite-plugin-sass-glob-import": "^5.0.0",
		"vue-i18n-extract": "^2.0.7"
	}
}
