{
	"title": "Vite WordPress Vue.js + Tailwind + Inertia.js Starter",
	"name": "wordpress-starter",
	"version": "4.1.1",
	"repository": "git@github.com:kirilldorozhynskyi/jdwordpress.git",
	"license": "GPL-2.0+",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"i18n:add": "vue-i18n-extract report --vueFiles './wp-content/themes/template/resources/Private/Vue/**/**/**/*.?(js|vue)' --languageFiles './wp-content/themes/template/resources/Private/Vue/util/lang/*.?(json|yml|yaml|js|ts)' --add",
		"i18n:remove": "vue-i18n-extract remove --vueFiles './wp-content/themes/template/resources/Private/Vue/**/*.?(js|vue)' --languageFiles './wp-content/themes/template/resources/Private/Vue/util/lang/*.?(json|yml|yaml|js|ts)' --remove"
	},
	"devDependencies": {
		"@peterek/vite-plugin-favicons": "^2.1.0",
		"@prettier/plugin-php": "^0.24.0",
		"@spiriit/vite-plugin-svg-spritemap": "^5.0.0",
		"@types/node": "^24.3.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"@zackad/prettier-plugin-twig": "^0.16.0",
		"laravel-vite-plugin": "^2.0.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sass": "^1.90.0",
		"svgo": "^4.0.0",
		"vite": "^7.1.3",
		"vite-plugin-mkcert": "^1.17.8",
		"vue-i18n-extract": "^2.0.7"
	},
	"dependencies": {
		"@inertiajs/vue3": "^2.1.2",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"aos": "^3.0.0-beta.6",
		"swiper": "^11.2.10",
		"tailwindcss": "^4.1.12",
		"vanilla-lazyload": "^19.1.3",
		"vue": "^3.5.18",
		"vue-i18n": "^11.1.11",
		"vue-scrollto": "^2.20.0"
	}
}
