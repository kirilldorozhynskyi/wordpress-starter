{
	"name": "justdev/wordpress-starter",
	"type": "project",
	"description": "jD WordPress starter",
	"homepage": "https://justdev.org",
	"license": "MIT",
	"keywords": [
		"WordPress",
		"Starter"
	],
	"version": "4.2.1",
	"require": {
		"php": ">=8.1",
		"composer/installers": "^2.3",
		"roots/wordpress": "^6.8.2",
		"vlucas/phpdotenv": "^5.6.2",
		"oscarotero/env": "^2.1.1",
		"roots/wp-config": "1.0.0",
		"ext-json": "*",
		"timber/timber": "^2.3.2",
		"roots/bedrock-autoloader": "^1.0.4",
		"wpengine/advanced-custom-fields-pro": "*",
		"yoast/wordpress-seo": "^25.5",
		"justdev/jd-support": "^2.1.1",
		"jdlocal/wp-rocket": "*",
		"pronamic/gravityforms": "^2.9.15",
		"kirilldorozhynskyi/jd-wp-sync-db": "*",
		"kirilldorozhynskyi/jd-wp-sync-db-media-files": "*",
		"kirilldorozhynskyi/wp-stage-switcher": "^2.2.1",
		"wpackagist-plugin/wp-mail-smtp": "^4.5.0",
		"wpackagist-plugin/redirection": "^5.5.2",
		"wpackagist-plugin/ewww-image-optimizer": "^8.2.0",
		"wpackagist-plugin/simple-custom-post-order": "^2.5.11",
		"wpackagist-plugin/query-monitor": "^3.18.0",
		"wpackagist-plugin/duplicate-post": "^4.5",
		"evo-mark/inertia-wordpress": "^0.8.4"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		},
		{
			"type": "vcs",
			"url": "git@github.com:kirilldorozhynskyi/wp-stage-switcher.git",
			"branch": "master"
		},
		{
			"type": "vcs",
			"url": "git@github.com:kirilldorozhynskyi/jd-wp-sync-db.git",
			"branch": "master"
		},
		{
			"type": "vcs",
			"url": "git@github.com:kirilldorozhynskyi/jd-wp-sync-db-media-files.git",
			"branch": "master"
		},
		{
			"type": "vcs",
			"url": "git@github.com:kirilldorozhynskyi/cm-gdpr-cookie-consent",
			"branch": "master"
		},
		{
			"type": "vcs",
			"url": "git@github.com:pronamic/gravityforms.git",
			"branch": "master"
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/gravityforms",
				"type": "wordpress-plugin",
				"version": "2.9.4.2",
				"dist": {
					"type": "zip",
					"url": "https://cms.justdev.link/gravityforms.zip"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/wp-sync-db",
				"type": "wordpress-plugin",
				"version": "1.6",
				"dist": {
					"type": "zip",
					"url": "https://cms.justdev.link/wp-sync-db.zip"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/wp-sync-db-media-files",
				"type": "wordpress-plugin",
				"version": "1.1.4b1",
				"dist": {
					"type": "zip",
					"url": "https://cms.justdev.link/wp-sync-db-media-files.zip"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/wp-rocket",
				"type": "wordpress-plugin",
				"version": "3.18",
				"dist": {
					"type": "zip",
					"url": "https://wp-rocket.me/download/355639/f25e5cc6/"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "composer",
			"url": "https://connect.advancedcustomfields.com"
		}
	],
	"extra": {
		"installer-paths": {
			"wp-content/mu-plugins/{$name}/": [
				"type:wordpress-muplugin"
			],
			"wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		},
		"wordpress-install-dir": "vendor/wordpress"
	},
	"autoload": {
		"psr-4": {
			"JDEV\\": "wp-content/themes/template/classes",
			"JDEVINT\\": "scripts/"
		}
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.11.3",
		"roave/security-advisories": "dev-latest"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"allow-plugins": {
			"composer/installers": true,
			"roots/wordpress-core-installer": true,
			"pivvenit/acf-pro-installer": true,
			"ffraenz/private-composer-installer": true
		}
	},
	"scripts": {
		"post-create-project-cmd": [
			"JDEVINT\\Installer::create",
			"@install"
		],
		"post-install-cmd": [
			"JDEVINT\\Installer::install",
			"rsync -av --exclude='.git' vendor/wordpress/ ./",
			"rm -rf vendor/wordpress/"
		],
		"post-update-cmd": [
			"rsync -av --exclude='.git' vendor/wordpress/ ./",
			"rm -rf vendor/wordpress/"
		],
		"install": [
			"git init -b master",
			"git add .",
			"git commit -m'[INT] Starter initial'",
			"cd wp-content/themes/template && npm i"
		]
	}
}
