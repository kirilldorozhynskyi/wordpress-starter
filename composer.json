{
	"name": "justdev/wordpress-starter",
	"type": "project",
	"description": "jD WordPress starter",
	"homepage": "https://justdev.org",
	"license": "MIT",
	"keywords": [
		"WordPress",
		"Starter"
	],
	"version": "3.1.0",
	"require": {
		"php": ">=8.1",
		"composer/installers": "^2.2",
		"roots/wordpress": "^6.5.2",
		"vlucas/phpdotenv": "^5.6",
		"oscarotero/env": "^2.1",
		"roots/wp-config": "1.0.0",
		"ext-json": "*",
		"timber/timber": "^2.2",
		"roots/bedrock-autoloader": "^1.0.4",
		"roots/wp-stage-switcher": "^2.2",
		"jdlocal/gravityforms": "2.8.5.1",
		"jdlocal/wp-rocket": "^1.0",
		"wpengine/advanced-custom-fields-pro": "*",
		"wpackagist-plugin/duplicate-post": "^4.5",
		"yoast/wordpress-seo": "^22.5",
		"wpackagist-plugin/wp-mail-smtp": "^4.0.1",
		"wpackagist-plugin/all-in-one-wp-security-and-firewall": "^5.2.9",
		"wpackagist-plugin/filebird": "^5.6.4",
		"wpackagist-plugin/safe-svg": "^2.2.4",
		"wpackagist-plugin/simple-301-redirects": "^2.0.7",
		"wpackagist-plugin/wp-smushit": "^3.14.1",
		"wpackagist-plugin/webp-express": "^0.25.6",
		"justdev/jd-support": "^1.1.6",
		"wpackagist-plugin/woocommerce": "^8.9.0",
		"kirilldorozhynskyi/jd-wp-sync-db": "*",
		"kirilldorozhynskyi/jd-wp-sync-db-media-files": "*",
		"kirilldorozhynskyi/cm-gdpr-cookie-consent": "*"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		},
		{
			"type": "vcs",
			"url": "git@github.com:kirilldorozhynskyi/jd-wp-sync-db.git",
			"branch": "master"
		},
		{
			"type": "vcs",
			"url": "git@github.com:kirilldorozhynskyi/jd-wp-sync-db-media-files.git",
			"branch": "master"
		},
		{
			"type": "vcs",
			"url": "git@github.com:kirilldorozhynskyi/cm-gdpr-cookie-consent",
			"branch": "master"
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/wp-rocket",
				"type": "wordpress-plugin",
				"version": "1.0",
				"dist": {
					"type": "zip",
					"url": "https://cms.justdev.link/wp-rocket.zip"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/gravityforms",
				"type": "wordpress-plugin",
				"version": "2.8.5.1",
				"dist": {
					"type": "zip",
					"url": "https://cms.justdev.link/gravityforms.zip"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/wp-sync-db",
				"type": "wordpress-plugin",
				"version": "1.6",
				"dist": {
					"type": "zip",
					"url": "https://cms.justdev.link/wp-sync-db.zip"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jdlocal/wp-sync-db-media-files",
				"type": "wordpress-plugin",
				"version": "1.1.4b1",
				"dist": {
					"type": "zip",
					"url": "https://cms.justdev.link/wp-sync-db-media-files.zip"
				},
				"require": {
					"composer/installers": "^2.0"
				}
			}
		},
		{
			"type": "composer",
			"url": "https://connect.advancedcustomfields.com"
		}
	],
	"extra": {
		"installer-paths": {
			"wp-content/mu-plugins/{$name}/": [
				"type:wordpress-muplugin"
			],
			"wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		},
		"wordpress-install-dir": "vendor/wordpress"
	},
	"autoload": {
		"psr-4": {
			"JDEV\\": "wp-content/themes/template/classes",
			"JDEVINT\\": "scripts/"
		}
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.7.1",
		"roave/security-advisories": "dev-latest"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"allow-plugins": {
			"composer/installers": true,
			"roots/wordpress-core-installer": true,
			"pivvenit/acf-pro-installer": true,
			"ffraenz/private-composer-installer": true
		}
	},
	"scripts": {
		"post-create-project-cmd": [
			"composer config extra.wordpress-install-dir '.'",
			"JDEVINT\\Installer::create",
			"@install"
		],
		"post-install-cmd": [
			"rsync -av --exclude='.git' vendor/wordpress/ ./",
			"rm -rf vendor/roots/wordpress/"
		],
		"post-update-cmd": [
			"JDEVINT\\Installer::install"
		],
		"install": [
			"git init",
			"npm i",
			"git commit -a -m '[INT] Starter initial'"
		]
	}
}
